<mvc:View xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m" xmlns:html="http://www.w3.org/1999/xhtml" controllerName="oj.orders.controller.Info">
	<App>
		<pages>
			<Page title="{i18n>infoTitle}">
				<content>
				    <List noDataText="Drop list items here" items="{/ProductSet}" id="list0">
				        <items>
				            <StandardListItem type="Navigation" title="{ProductID} - {Name}" description="{Description}" icon="sap-icon://picture" id="item0"/>
				        </items>
				    </List>
				</content>
			</Page>
		</pages>
	</App>
</mvc:View>